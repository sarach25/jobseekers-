{% extends "MyAppUserBundle::layout7.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}
{% block contenu %}
    <head>

        <link href="{{ asset('MyApp/css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('MyApp/css/style.css') }}" rel="stylesheet">
        <link href="{{ asset('MyApp/css/style3.css') }}" rel="stylesheet">


    </head>


    <h1>Ajout Entreprise</h1>

    <form  method="POST" >





        <div class="form-group row">
         <label for="inputnom" class="col-sm-4 control-label"> Nom entreprise</label>
            <div class="col-sm-6 form-group">
            <input type="text" placeholder="le Nom d'entreprise.." class="form-control"id="fos_user_registration_form_nom" name="nomentreprise" required="required" maxlength="255">
        </div>
        </div>



        <div class="form-group row">
        <label for="inputnom" class="col-sm-4 control-label"> domaine</label>
            <div class="col-sm-6 form-group">
                <input type="text" placeholder="Ecrire le domaine ..." class="form-control"  name="domaine" required="required" maxlength="255">
            </div>

        </div>






        <div class="form-group row">
            <label for="inputnom" class="col-sm-4 control-label"> Adresse</label>
            <div class="col-sm-6 form-group">
                <input type="text" placeholder="Ecrice l'adresse  ..." class="form-control" id="fos_user_registration_form_prenom" name="adresse" required="required" maxlength="255">
            </div><br>

        </div>


        <div class="form-group row">
            <label for="inputnom" class="col-sm-4 control-label"> Image</label>
            <div class="col-sm-6 form-group">


                <input type="file"  placeholder=" ..." class="form-control" id="IMAGE" name="image" >
            </div>

        </div>






        <input type="submit" value="ajouter" class="btn btn-default"/>














    </form>


{% endblock contenu %}